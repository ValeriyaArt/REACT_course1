));\r\n    })\r\n}\r\n\r\nexport const updateStatus = (status) => (dispatch) => {\r\n    profileAPI.updateStatus(status).\r\n    then(response => {\r\n        if (response.data.resultCode === 0) {\r\n            dispatch(setStatus(status));\r\n        }\r\n    })\r\n}\r\n\r\nexport default profileReducer","C:\\Users\\User\\easy_react\\REACT_course1\\my-app\\src\\components\\Profile\\Profile.js",[],"C:\\Users\\User\\easy_react\\REACT_course1\\my-app\\src\\components\\Users\\UsersContainer.js",[],"C:\\Users\\User\\easy_react\\REACT_course1\\my-app\\src\\components\\Header\\Header.js",["254"],"C:\\Users\\User\\easy_react\\REACT_course1\\my-app\\src\\components\\Navbar\\Navbar.js",[],"C:\\Users\\User\\easy_react\\REACT_course1\\my-app\\src\\components\\Dialogs\\DialogsContainer.js",[],"C:\\Users\\User\\easy_react\\REACT_course1\\my-app\\src\\components\\Users\\Users.js",["255"],"import userPhoto from \"../../assets/images/user.png\";\r\nimport styles from \"./Users.module.css\";\r\nimport React from \"react\";\r\nimport {NavLink} from \"react-router-dom\";\r\n\r\n\r\nlet Users = (props) => {\r\n    let pagesCount = Math.ceil(props.totalUsersCount / props.pageSize);\r\n\r\n    let pages = [];\r\n    for (let i = 1; i <= pagesCount; i++) {\r\n        pages.push(i);\r\n    }\r\n    console.log(props)\r\n    return (\r\n\r\n        <div>\r\n            <div>\r\n                {pages.map(p => {\r\n                        return <span className={props.currentPage === p && styles.selectedPage}\r\n                                     onClick={(e) => {\r\n                                         props.onPageChanged(p)\r\n                                     }}>{p}</span>\r\n                    }\r\n                )\r\n                }\r\n            </div>\r\n            {\r\n                props.users.map(u => <div key={u.id}>\r\n                <span>\r\n                    <div>\r\n                        <NavLink to={'/profile/' + u.id}>\r\n                        <img s